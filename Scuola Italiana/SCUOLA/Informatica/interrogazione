#include <iostream>
using namespace std;
int main()
{
  int c=0;
  string s, s1;
  std::cout << "inserisci una stinga: ";
  std::cin >> s;
  std::cout << "inserisci una seconda tringa: ";
  std::cin >> s1;
  for(int i=0;i<s.length();i++)
  {
    for (int j=0;j<s1.length();j++)
    {
      if (s[i]==s1[j])
      {
        std::cout << s[i] << '\n';
        c++;
      }
    }
  }
  std::cout << "sono stati trovati "<<c<<" caratteri uguali" << '\n';
  return 0;
}
